<Window x:Class="Ateler.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ateler"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame Name="MainFrame" ContentRendered="MainframeContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" FontFamily="times new roman" Foreground="White" Background="Black" Name="back" Click="Button_Click" Grid.Row="0" Height="20" Width="80" HorizontalAlignment="Right" Margin="0 200 0 0"></Button>
    </Grid>
</Window>
